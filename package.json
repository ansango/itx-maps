{
  "name": "vr-ts-starter",
  "version": "0.0.1",
  "scripts": {
    "start": "ntl -s 12 -e start prepare",
    "cli": "plop --plopfile cli/index.js",
    "dev": "vite",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "npm run clean:t && jest --maxWorkers=50%",
    "test:w": "jest --watch --maxWorkers=25%",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "clean:t": "rimraf coverage",
    "clean:d": "rimraf dist",
    "clean:all": "rimraf dist && rimraf storybook-static && rimraf coverage",
    "prepare": "husky install"
  },
  "ntl": {
    "descriptions": {
      "cli": "Start CLI",
      "dev": "Start development server",
      "lint": "Lint code",
      "test": "Run tests",
      "test:w": "Run tests in watch mode",
      "preview": "Start preview server",
      "build": "Build project",
      "clean:t": "Clean test coverage",
      "clean:d": "Clean dist",
      "clean:all": "Clean all"
    }
  },
  "dependencies": {
    "@reach/combobox": "^0.16.5",
    "@react-google-maps/api": "^2.7.0",
    "@reduxjs/toolkit": "^1.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "use-places-autocomplete": "^1.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@googlemaps/jest-mocks": "^2.6.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.11",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@types/supercluster": "^5.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "autoprefixer": "^10.4.2",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "chalk": "^5.0.0",
    "clear": "^0.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "figlet": "^1.5.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "ntl": "^5.1.0",
    "plop": "^3.0.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "redux-mock-store": "^1.5.4",
    "tailwindcss": "^3.0.23",
